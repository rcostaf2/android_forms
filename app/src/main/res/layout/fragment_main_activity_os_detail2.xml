<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.aroos.metacom.activities.others.MainActivityOsDetail$PlaceholderFragment">


    <TextView
        android:id="@+id/section_2label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Endereço"
        android:layout_alignParentLeft="true"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editTextLogr"
        android:layout_below="@+id/section_2label2"
        android:inputType="textMultiLine"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:editable="false"
        android:enabled="false" />


    <TextView
    android:id="@+id/section_2label7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Bairro"
    android:layout_alignParentLeft="true"
    android:layout_below="@+id/editTextLogr"
    android:textStyle="bold"
    android:textColor="@color/colorPrimary" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editTextBairro"
        android:layout_below="@+id/section_2label7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:editable="false"
        android:enabled="false" />
    <TextView
        android:id="@+id/section_2label8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cidade"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/editTextBairro"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editTextCidade"
        android:layout_below="@+id/section_2label8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:editable="false"
        android:enabled="false" />
    <TextView
        android:id="@+id/section_2label9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UF"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/editTextCidade"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editTextUf"
        android:layout_below="@+id/section_2label9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:editable="false"
        android:enabled="false" />
    <TextView
        android:id="@+id/section_2label10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Referência"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/editTextUf"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary" />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editTextreferencia"
        android:layout_below="@+id/section_2label10"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:editable="false"
        android:enabled="false" />
    <TextView
        android:id="@+id/section_label134"
        android:layout_width="wrap_content"
        android:text="     "
        android:layout_height="wrap_content"
        android:layout_below="@+id/editTextreferencia"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary"/>
    <TextView
        android:id="@+id/section_label135"
        android:layout_width="wrap_content"
        android:text="     "
        android:layout_height="wrap_content"
        android:layout_below="@+id/section_label134"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary"/>
</RelativeLayout>
</ScrollView>